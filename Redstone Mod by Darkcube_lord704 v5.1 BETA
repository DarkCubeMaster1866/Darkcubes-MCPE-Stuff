function useItem(x, y, z, item, block, side)
{
//redstone creeper
if(item == 161)
{
Level.spawnMob(x, y+1, z, 33, "mob/creeper_redstone.png");
addItemInventory(item,-1,0);
}
}
   
//Redstone Mob Drops
function deathHook(murderer, victim)
{
if(Entity.getMobSkin(victim)=="mob/creeper_redstone.png") 
{
Level.dropItem(Entity.getX(victim), Entity.getY(victim) + 1, Entity.getZ(victim), 0.5, 331,2, 0);
}
}
 
 
 
 
